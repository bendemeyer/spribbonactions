<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
    <CustomAction   Id="BeenJammin.SharePoint.CustomRibbonActions.AudioVideo"   Location="CommandUI.Ribbon">
        <CommandUIExtension>
            <CommandUIDefinitions>
                <CommandUIDefinition Location="Ribbon.EditingTools.CPInsert.Media.Controls._children">
                    <Button Id="BeenJammin.SharePoint.CustomRibbonActions.MediaGroup.Video" Command="BeenJammin.MediaGroup.Command.Video" Sequence="40" Image16by16="/_layouts/images/BeenJammin.SharePoint.CustomRibbonActions/youtube-ribbonicon.gif" Image32by32="/_layouts/images/BeenJammin.SharePoint.CustomRibbonActions/youtube-ribbonicon.gif" Description="Include the Video file." LabelText="Youtube Embed" TemplateAlias="o1" />
                </CommandUIDefinition>
            </CommandUIDefinitions>
            <CommandUIHandlers>
                <CommandUIHandler Command="BeenJammin.MediaGroup.Command.Video" CommandAction="javascript:InsertVideo()" />
            </CommandUIHandlers>
        </CommandUIExtension>
    </CustomAction>
    <CustomAction Id="BeenJammin.SharePoint.RTEActions.Scripts" Location="ScriptLink" ScriptSrc="BeenJammin.SharePoint.CustomRibbonActions/RibbonActions.js"/>
</Elements>
